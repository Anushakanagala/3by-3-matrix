#include<stdio.h>
main()
{
	int a[3][3];
	int i,j;
	printf("enter 9 values: \n");
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}
	printf("\n displaying values:\n");
	for(i=0;i<3;++i)
	{
		for(j=0;j<3;++j)
		{
			printf("a[%d][%d]=%d\n",i,j,a[i][j]);
		}
	}
	 printf("\nThe matrix is : \n");
  for(i=0;i<3;i++)
  {
      printf("\n");
      for(j=0;j<3;j++)
           printf("%d\t",a[i][j]);
  }
 printf("\n\n");
}
